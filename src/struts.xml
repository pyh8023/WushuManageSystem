<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="true"/>
	
	<!--解决乱码    -->
    <constant name="struts.i18n.encoding" value="UTF-8"></constant>
    
	<package name="struts"  namespace="/"  extends="json-default">
		
		<action name="admin" class="com.pan.competition.action.AdminAction">
			<result name="success" type="json">
				<param name="root">result</param>
			</result>
		</action>
		<action name="user"  class="com.pan.competition.action.UserAction"> 
			<result name="success">/test/test1.jsp</result>
			<result name="failed">/test/test.jsp</result>
		</action>
   </package>
</struts>